<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lanbots.activity01"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.lanbots.activity01.MainActivity"
            android:label="@string/app_name" >
            <!-- 设置MainActivity为启动Activity -->
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- 注册secondActivity -->
        <activity
            android:name="com.lanbots.activity01.SecondActivity"
            android:label="@string/app_name"
            >
            
        </activity>
        
        <activity
            android:name="com.lanbots.activity01.ThirdActivity"
            android:label="@string/app_name"
            >
            
        </activity>
    </application>
<uses-permission android:name="android.permission.CALL_PHONE"/>
</manifest>
